{
  "name": "@deepgram-web/www",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "predev": "yarn clean && yarn content && yarn media",
    "dev": "astro dev --experimental-integrations",
    "prebuild": "yarn content && yarn media",
    "build": "astro build --experimental-integrations",
    "preview": "astro preview",
    "clean": "rm -rf ./src/content/ && rm -rf ./public/uploads",
    "content": "cp -r ../content/www/ ./src/content/",
    "media": "cp -r ../media/ ./public/uploads/"
  },
  "devDependencies": {
    "astro": "^1.0.0-beta.29",
    "vue": "^3.2.33"
  },
  "dependencies": {
    "@types/node": "^17.0.42",
    "glob": "^8.0.3"
  }
}
