---
import Default from "./Default.astro";
import PrimarySection from "../shared/components/layout/PrimarySection.astro";
import Prose from "./Prose.astro";
import TableOfContents from "../shared/components/nav/TableOfContents.astro";
import BreadCrumb from "../shared/components/nav/BreadCrumb.astro";
const { headings } = Astro.props
---

<Default>
	<PrimarySection class="max-w-screen-xl mb-[5rem] mx-auto px-6 lg:px-10 pt-[3rem]">
		<BreadCrumb/>
		<div class="flex">
			<Prose>
					<slot />
			</Prose>
			<div class="ml-8">
				<TableOfContents headings={headings} docsType={'USE CASE'} />
			</div>
		</div>
	</PrimarySection>
	

</Default>

