---
import AbstractPromo from "./AbstractPromo.astro";
import PromoForm from "./PromoForm.astro";

const { date, title, formProps, buttonText, strapline, backgroundImage, theme, ...rest } = Astro.props;
---

<AbstractPromo theme={theme} title={title} strapline={strapline} backgroundImage={backgroundImage} {...rest}>
	<PromoForm theme={theme} formProps={formProps} buttonText={buttonText} />
</AbstractPromo>
